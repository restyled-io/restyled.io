/robots.txt RobotsR GET
/static StaticR Static appStatic

/ HomeR GET
/signups SignupR POST
/webhooks WebhooksR POST
/privacy-policy PrivacyPolicyR GET

/gh/#OwnerName OwnerP:
  /repos ReposP:
    / ReposR GET
    /#RepoName RepoP:
      / RepoR GET
      /pulls RepoPullsP:
        /#Int RepoPullP:
          / RepoPullR GET
          /jobs RepoPullJobsP:
            / RepoPullJobsR GET
      /jobs RepoJobsP:
        / RepoJobsR GET
        /#JobId RepoJobR GET

/admin AdminR GET
/admin AdminP:
  /jobs AdminJobsP:
    / AdminJobsR POST
    /new AdminJobsNewR GET
    !/#JobId AdminJobR DELETE
  /repos AdminReposP:
    / AdminReposR GET
    /#RepoId AdminRepoP:
      / AdminRepoR PATCH
      /jobs AdminRepoJobsR GET
  /signups AdminSignupsR GET
